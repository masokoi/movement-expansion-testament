{
	"schemaVersion": 1,
	"id": "movement-expansion-testament",
	"version": "${version}",
	"name": "Movement Expansion Testament",
	"description": "Expands movement with My own will, blood tears and sweat. Check the config file & controls tab for info!",
	"authors": [
		"Masoki"
	],
	"contact": {
		"homepage": "",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/movement-expansion-testament/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "masoki.met.MovementExpansionTestament",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "masoki.met.MovementExpansionTestamentClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "masoki.met.MovementExpansionTestamentDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"movement-expansion-testament.mixins.json",
		{
			"config": "movement-expansion-testament.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.4",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}